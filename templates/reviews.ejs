                <title>Fourth star</title>
                <path
                    d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z">
                </path>
            </svg>
            <svg aria-hidden="true" class="w-5 h-5 text-gray-300 dark:text-gray-500" fill="currentColor"
                viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                <title>Fifth star</title>
                <path
                    d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z">
                </path>
            </svg>
            <p class="ml-2 text-sm font-medium text-gray-900 dark:text-white">
                <%=averageRating%> out of 5
            </p>
        </div>
        <p class="lg:text-left text-center text-sm font-medium text-gray-500 dark:text-gray-400">
            <span id="totalRating">
                <%=event.reviews.length%>
            </span> ratings
        </p>
        <div class="progress flex flex-col items-center lg:block">
            <%starArr.forEach((percentage,i)=>{%>
                <div class="w-full flex lg:justify-start justify-center items-center mt-4">
                    <span class="text-sm font-medium text-blue-600 dark:text-blue-500">
                        <%=i+1%> star
                    </span>
                    <div class="w-2/4 h-5 mx-4 bg-gray-200 rounded dark:bg-gray-700">
                        <div class="h-5 bg-yellow-400 rounded w-[<%=percentage%>%]">

                        </div>
                    </div>
                    <span class="text-sm font-medium text-blue-600 dark:text-blue-500 singleStar">
                        <%=percentage%>%
                    </span>
                </div>
                <% })%>
        </div>
    </div>
    <div class="w-[50%] mt-10 min-h-[18rem] flex items-center">
        <%if(event.reviews.length>0){%>
            <article class="h-44 overflow-hidden ">
                <div class="flex items-center mb-4 space-x-4">
                    <img class="w-10 h-10 rounded-full"
                        src="https://images.unsplash.com/photo-1472099645785-5658abf4ff4e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80"
                        alt="" alt="">
                    <div class="space-y-1 font-medium dark:text-white">
                        <p>
                            <%=event.reviews[0].name%> <time datetime="2014-08-16 19:00"
                                    class="block text-sm xtext-gray-500 dark:text-gray-400">Joined on August 2014</time>
                        </p>
                    </div>
                </div>

                <div class="reviews flex items-center mb-1">
                    <% for (var i=0; i < event.reviews[0].rating; i++) { %>
                        <i class="fas text-yellow-400 fa-star"></i>
                        <% } %>
                            <% for (var i=event.reviews[0].rating; i < 5; i++) { %>
                                <i class="far text-yellow-400 fa-star"></i>
                                <% } %><!--Empty star-->
                </div>
                <footer class="mb-5 text-sm text-gray-500 dark:text-gray-400">
                    <p>Reviewed on <time datetime="2017-03-03 19:00">March 3, 2017</time></p>
                </footer>
                <p class="mb-2 font-light text-gray-500 dark:text-gray-400">
                    <%=event.reviews[0].comment%>
                </p>


                <!-- <aside >
                    <p class="mt-1 text-xs text-gray-500 dark:text-gray-400">19 people found this helpful</p>
                    <div class="flex items-center mt-3 space-x-3 divide-x divide-gray-200 dark:divide-gray-600">
                        <a href="#"
                            class="text-gray-900 bg-white border border-gray-300 focus:outline-none hover:bg-gray-100 focus:ring-4 focus:ring-gray-200 font-medium rounded-lg text-xs px-2 py-1.5 dark:bg-gray-800 dark:text-white dark:border-gray-600 dark:hover:bg-gray-700 dark:hover:border-gray-600 dark:focus:ring-gray-700">Helpful</a>
                        <a href="#"
                            class="pl-4 text-sm font-medium text-blue-600 hover:underline dark:text-blue-500">Report
                            abuse</a>
                    </div>
                </aside> -->
            </article>
            <button
                class="hidden readMore  mb-5 text-sm font-medium text-blue-600 hover:underline dark:text-blue-500">Read
                more</button>
            <%}%>
    </div>
</div>
<div class=" w-[80vw] m-auto flex items-center justify-center ">
    <div id="reviewBox" class="w-[100%] grid   grid-cols-2 overflow-hidden h-[55rem]">

        <%if(event.reviews.length>0){%>
            <%event.reviews.forEach((review,i)=>{%>
                <%if(i!==0){%>
                    <div>
                        <article class="h-44 overflow-hidden">
                            <div class="flex items-center mb-4 space-x-4">
                                <img class="w-10 h-10 rounded-full"
                                    src="https://images.unsplash.com/photo-1472099645785-5658abf4ff4e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80"
                                    alt="" alt="">
                                <div class="space-y-1 font-medium dark:text-white">
                                    <p>
                                        <%=review.name%> <time datetime="2014-08-16 19:00"
                                                class="block text-sm xtext-gray-500 dark:text-gray-400">Joined on August
                                                2014</time>
                                    </p>
                                </div>
                            </div>

                            <div class="reviews flex items-center mb-1">
                                <% for (var i=0; i < review.rating; i++) { %>
                                    <i class="fas text-yellow-400 fa-star"></i>
                                    <% } %>
                                        <% for (var i=review.rating; i < 5; i++) { %>
                                            <i class="far text-yellow-400 fa-star"></i>
                                            <% } %><!--Empty star-->
                            </div>
                            <footer class="mb-5 text-sm text-gray-500 dark:text-gray-400">
                                <p>Reviewed on <time datetime="2017-03-03 19:00">March 3, 2017</time></p>
                            </footer>
                            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">
                                <%=review.comment%>
                            </p>


                            <!-- <aside >
                    <p class="mt-1 text-xs text-gray-500 dark:text-gray-400">19 people found this helpful</p>
                    <div class="flex items-center mt-3 space-x-3 divide-x divide-gray-200 dark:divide-gray-600">
