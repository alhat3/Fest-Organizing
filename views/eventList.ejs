<%if(data.length>0){%>

    <section class="container mx-auto p-10 md:py-12 px-0 md:p-8 md:px-0">
        <section
            class="p-5 md:p-0 grid grid-cols-1 sm:grid-cols-1 md:grid-cols-2 xl:grid-cols-3 2xl:grid-cols-4 gap-10 items-start ">
            <%data.forEach((event)=>{%>
                <section
                    class="p-5 py-5 bg-purple-50 text-center transform duration-500 hover:-translate-y-2 cursor-pointer">
                    <!-- <img src="../static/img/event-img/<%=event.images[0].url%>" alt=""> -->
                    <div class="relative h-64 w-full overflow-hidden rounded-lg bg-white lg:aspect-w-1 ">
                        <img src="../static/img/event-img/<%=event.images[0].url%>"
                            alt="Desk with leather desk pad, walnut desk organizer, wireless keyboard and mouse, and porcelain mug."
                            class="h-full w-full object-cover object-center">
                    </div>
                    <h1 class="text-2xl my-3">
                        <%=event.name%>
                    </h1>
                    <p class="mb-3">
                        <%=event.description%>
                    </p>
                    <h2 class="font-semibold mb-3">
                        <%=event.price%>rs
                    </h2>
                    <a href="/api/v1/event/<%=event.id%>"
                        class="p-2 px-6 bg-purple-500 text-white rounded-md hover:bg-purple-600">Register
                        Now</a>
                </section>
                <%})%>

        </section>
    </section>
    <%}else{%>
        <h1 class="m-auto text-center text-3xl font-bold mt-16 text-purple-600">Coming Soon</h1>
        <%}%>